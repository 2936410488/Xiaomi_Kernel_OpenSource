&u2port0 {
	mediatek,eye-term = <4>;
	mediatek,eye-vrt = <4>;
	mediatek,rev6 = <2>;
	mediatek,discth = <0xb>;
	mediatek,eye-term-host = <4>;
	mediatek,eye-vrt-host = <4>;
	mediatek,rev6-host = <2>;
};

&odm {
	simtray {
		compatible = "xiaomi,simtray-status";
		status-gpio =	<&pio 42 0>;
	};
};
&i2c9 {
	clock-frequency = <400000>;
	status = "okay";

	sc8551_i2c9@66 {
		compatible = "sc8551_i2c9";
		reg = <0x66>;
		sc8551_irq_gpio = <&pio 13 0x0>;
		ac_ovp = <13000>;
		bus_ovp = <12300>;
		bus_ocp = <4500>;
		bat_ovp = <5000>;
		bat_ocp = <8000>;
	};

        sc8561_master@64 {
        	compatible = "sc8561_master";
        	reg = <0x64>;
        	sc8561_irq_gpio = <&pio 13 0x0>;
	};
};

&i2c7 {
	clock-frequency = <400000>;
	status = "okay";
	bq28z610@55 {
		compatible = "bq28z610";
		reg = <0x55>;
		enable_shutdown_delay;
		normal_shutdown_vbat_1s = <3400>;
		critical_shutdown_vbat_1s = <3340>;
		report_full_rsoc_1s = <9700>;
		soc_gap_1s = <2>;
		soc_decimal_rate = <
			0  38
			10 35
			20 33
			30 33
			40 33
			50 33
			60 33
			70 30
			80 25
			90 20
			95 10 >;
	};

	sc8551_i2c7@66 {
		compatible = "sc8551_i2c7";
		reg = <0x66>;
		sc8551_irq_gpio = <&pio 14 0x0>;
		ac_ovp = <13000>;
		bus_ovp = <12300>;
		bus_ocp = <4500>;
		bat_ovp = <5000>;
		bat_ocp = <8000>;
	};

        sc8561_slave@65 {
		compatible = "sc8561_slave";
		reg = <0x65>;
		sc8561_irq_gpio = <&pio 14 0x0>;
	};
};

