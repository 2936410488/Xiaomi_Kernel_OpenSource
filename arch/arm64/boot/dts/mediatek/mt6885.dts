	pio: pinctrl {
		compatible = "mediatek,mt6885-pinctrl";
		reg_bases = <&gpio>,
			    <&iocfg_rm>,
			    <&iocfg_bm>,
			    <&iocfg_lm>,
			    <&iocfg_lb>,
			    <&iocfg_rt>,
			    <&iocfg_lt>,
			    <&iocfg_tm>;
		pins-are-numbered;
		gpio-controller;
		gpio-ranges = <&pio 0 0 219>;
		#gpio-cells = <2>;
		#interrupt-cells = <4>;
	};

	msdc0: msdc@11230000 {
